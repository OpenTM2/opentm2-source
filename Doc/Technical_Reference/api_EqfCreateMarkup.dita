  <refbody>
<!-- xxx  -->
    <section>
      <title>Purpose</title>
      <indexterm>EqfCreateMarkup</indexterm>
      <indexterm>programming interface calls<indexterm>EqfCreateMarkup</indexterm></indexterm>
      <p>The API call <i>EqfCreateMarkup</i> creates an internal markup table (*.TBL) from an external markup table (*.TBX).</p>
    </section>
<!-- xxx  -->

<!-- xxx  -->    
    <refsyn>
      <title>Format</title>
      <syntaxdiagram>
        <groupseq>
          <kwd>usRC</kwd>
          <oper> = </oper>
          <kwd>EqfCreatMarkup</kwd>
          <delim>(</delim>
          <var>hSession</var>
          <sep>,</sep>
        </groupseq>
        <groupseq>
          <var>pszInfile</var>
          <sep>,</sep>
        </groupseq>
        <groupseq>
          <var>pszOutfile</var>
          <sep>,</sep>
        </groupseq>
        <groupseq>
          <delim>)</delim>
          <sep>;</sep>
        </groupseq>
      </syntaxdiagram>
    </refsyn>
<!-- xxx  -->

