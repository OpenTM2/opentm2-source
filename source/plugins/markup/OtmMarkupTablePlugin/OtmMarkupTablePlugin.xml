<markups>
  <plugin>
    <name>OtmMarkupTablePlugin</name>
    <shortName>OTM</shortName>
	<shortDescription>OTM Markup Table Plugin</shortDescription>
	<longDescription>This is the markup table plugin for the OpenTM2 markup tables</longDescription>
	<version>1.0</version>
	<supplier>OpenTM2</supplier>
    <tableDirectory>TABLE</tableDirectory>
    <userExitDirectory>BIN</userExitDirectory>
    <deletable>no</deletable>
    <exportable>yes</exportable>
    <importable>no</importable>
    <protected>yes</protected>
  </plugin>

  <!--  Plain text markups  -->

  <markup>
    <name>OTMANSI</name>
    <shortDescription>Plain text - ANSI encoding</shortDescription>
    <longDescription>Markup for plain text documents which are encoded in ANSI</longDescription>
    <version>1.0</version>
    <table>OTMANSI.TBL</table>
    <userExit>none</userExit>
    <files>none</files>
  </markup>

