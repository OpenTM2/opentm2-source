ID_FOL_DELHLOG_DLG DIALOG LOADONCALL MOVEABLE DISCARDABLE  9, 12, 362, 180
STYLE DS_CONTEXTHELP | DS_MODALFRAME | WS_VISIBLE | WS_CAPTION | WS_SYSMENU | WS_BORDER
CAPTION "History log delete confirmation"
FONT 8 "MS Sans Serif"
BEGIN

  CONTROL "Warning!", -1,
    STATIC, SS_LEFT | WS_GROUP | WS_VISIBLE, 6, 15, 200, 9
    
  CONTROL "When you delete the history log file all information necessary for calculation reports will be deleted and \
no history of the folder and its documents is available anymore.\n", -1,
    STATIC, SS_LEFT | WS_GROUP | WS_VISIBLE, 6, 34, 340, 18
    
  CONTROL "Only delete the history log file when you re-use the folder for new and independend shipments or for different translation projects.", -1,
    STATIC, SS_LEFT | WS_GROUP | WS_VISIBLE, 6, 62, 340, 18
    
  CONTROL "Your user ID:", -1,
    STATIC, SS_LEFT | WS_GROUP | WS_VISIBLE, 6, 90, 80, 9

  CONTROL "", ID_FOL_DELHLOG_USERID_EF,
    EDIT,
    ES_NOHIDESEL | WS_BORDER | WS_GROUP | WS_TABSTOP | WS_VISIBLE,
    200, 88, 80, 12
    
  CONTROL "Confirmation text:", -1,
    STATIC, SS_LEFT | WS_GROUP | WS_VISIBLE, 6, 110, 80, 9
    
  CONTROL "", ID_FOL_DELHLOG_CAPTCHA_EF,
    STATIC, SS_LEFT | WS_GROUP | WS_VISIBLE, 200, 110, 80, 9
    
  CONTROL "To confirm the delete enter the six characters of the confirmation text here:", -1,
    STATIC, SS_LEFT | WS_GROUP | WS_VISIBLE, 6, 132, 170, 18
    
  CONTROL "", ID_FOL_DELHLOG_CONFIRM_EF,
    EDIT,
    ES_NOHIDESEL | WS_BORDER | WS_GROUP | WS_TABSTOP | WS_VISIBLE,
    200, 132, 80, 12
    
  CONTROL "Delete history log", ID_FOL_DELHLOG_DEL_PB,
    BUTTON,
    BS_PUSHBUTTON | WS_TABSTOP | WS_VISIBLE | WS_GROUP,
    29, 162, 80, 15
  CONTROL "Cancel", ID_FOL_DELHLOG_CANCEL_PB,
    BUTTON,
    BS_DEFPUSHBUTTON | WS_TABSTOP | WS_VISIBLE,
    171, 162, 38, 15
  CONTROL "Help", ID_FOL_DELHLOG_HELP_PB,
    EQFHELPBUTTON,
    WS_TABSTOP | WS_VISIBLE,
    275, 162, 38, 15
END
