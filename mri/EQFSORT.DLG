                    WS_VSCROLL | WS_TABSTOP
    CONTROL         "Ascending",ID_SORT_ASCENDING3_RB,"Button",
                    BS_AUTORADIOBUTTON,136,98,92,10
    CONTROL         "Descending",ID_SORT_DESCENDING3_RB,"Button",
                    BS_AUTORADIOBUTTON,136,111,92,10

