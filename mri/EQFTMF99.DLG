    BUTTON,
    BS_AUTOCHECKBOX | WS_GROUP | WS_TABSTOP | WS_VISIBLE,
    12, 194, 86, 12
  CONTROL "", ID_TM_QUERY_DOC_STATIC, 
    STATIC,
    SS_LEFT | WS_VISIBLE,
    107, 197, 77, 9

  CONTROL STR_TM_QUERY_DOC_PB, ID_TM_QUERY_DOC_PB, 
    BUTTON,
    BS_PUSHBUTTON | WS_TABSTOP | WS_VISIBLE,
    189, 191, 56, 15
