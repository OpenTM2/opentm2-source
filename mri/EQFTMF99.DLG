    BUTTON,
    BS_PUSHBUTTON | WS_TABSTOP | WS_VISIBLE,
    127, 246, 38, 15

  CONTROL STR_TM_QUERY_HELP_PB, ID_TM_QUERY_HELP_PB, 
    EQFHELPBUTTON,
    WS_TABSTOP | WS_VISIBLE,
    212, 246, 38, 15
