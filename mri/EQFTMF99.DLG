    STATIC,
    SS_LEFT | WS_VISIBLE,
    98, 179, 86, 9

  CONTROL STR_TM_QUERY_DATE_PB, ID_TM_QUERY_DATE_PB, 
    BUTTON,
    BS_PUSHBUTTON | WS_TABSTOP | WS_VISIBLE,
    189, 173, 56, 15
