    STATIC,
    SS_LEFT | WS_GROUP | WS_VISIBLE,
    181, 230, 34, 9
  CONTROL "", ID_TM_QUERY_PMSEGS_EF, 
    EDIT,
    ES_CENTER | WS_BORDER | WS_TABSTOP | WS_VISIBLE,
    220, 229, 17, 12

  CONTROL STR_TM_QUERY_PMSEGS2, ID_TM_QUERY_PMSEGS2, 
    STATIC,
    SS_LEFT | WS_GROUP | WS_VISIBLE,
    242, 230, 34, 9
